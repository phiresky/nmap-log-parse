{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"noImplicitAny": true,
		"sourceMap": true,
		"jsx": "react",
		"strict": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"outDir": "bin",
		"allowSyntheticDefaultImports": true
	}
}
